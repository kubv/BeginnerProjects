def isYearLeap(year):
#
# put your code here
#
    if year % 4 == 0 :  #needs to be divisible by 4
        if year % 100 == 0: #if its divisible by 100
            if year % 400 == 0: # it needs to be divisible by 400
                return True
            else:
                return False
        else:  # divisible by 4 but not by 100
            return True
    else: #not divisible by 4
        return False
        

testData = [1900, 2000, 2016, 1987]
testResults = [False, True, True, False]
for i in range(len(testData)):
	yr = testData[i]
	print(yr,"->",end="")
	result = isYearLeap(yr)
	print (result)
	if result == testResults[i]:
		print("OK")
	else:
		print("Failed")
